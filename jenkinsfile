pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo 'Building..'
				git pull  https://github.com/Nithesh-b/lab.git master
				git checkout master
				git reset HEAD^
				#git remote -v
				git remote set-url origin git@github.com:nithesh-b/lab.git
				git push -f origin master --no-verify
            }
        }
    }
}